{
  "entrypoint": "index.html",
  "shell": "src/sh-app/sh-app.html",
  "fragments": [],
  "sources": [
    "images/**/*",
    "src/**/*",
    "index.html",
    "src/sh-app/sh-app.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "node_modules/blueimp-load-image/*.js",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/*.js.map",
    "!bower_components/webcomponentsjs/gulpfile.js"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es5-bundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}